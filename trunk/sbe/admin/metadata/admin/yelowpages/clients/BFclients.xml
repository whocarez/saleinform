<?xml version="1.0" encoding="UTF-8"?>
<BizForm BizDataObj="admin.yelowpages.clients.BOclients" Class="BizForm" Description="Add description of BizForm" InheritFrom="" Name="BFclients" Package="admin.yelowpages.clients" PageSize="28" SearchRule="" Title="Справочник фирм" jsClass="jbForm">
	<DisplayModes>
		<Mode DataFormat="array" Name="QUERY" TemplateFile="edit.tpl"/>
		<Mode DataFormat="array" Name="EDIT" TemplateFile="edit.tpl"/>
		<Mode DataFormat="array" Name="NEW" TemplateFile="edit.tpl"/>
		<Mode DataFormat="block" FormatStyle="tbl" Name="READ" TemplateFile="list.tpl"/>
	</DisplayModes>
	<BizCtrlList>
		<BizCtrl DisplayName="ID" FieldName="Id" Name="bctrl_Id" DisplayMode="READ" Sortable="Y" Required="Y"/>
		<BizCtrl DisplayName="Форма собственности" FieldName="_urforms_rid" Name="bctrl__urforms_rid" SelectFrom="admin.yelowpages.urforms.BOurforms[name:Id],[archive]=0" Required="Y" DisplayMode="EDIT|NEW"/>
		<BizCtrl DisplayName="Наименование" FieldName="name" Name="bctrl_name" Sortable="Y" Required="Y" Width="300" DisplayMode="NEW|EDIT|QUERY"/>
		<BizCtrl DisplayName="Наименование" FieldName="name" Name="bctrl_name_full" Sortable="Y" Required="Y" Width="300" DisplayMode="READ"/>
		<!-- 
		<BizCtrl DisplayName="Наименование" FieldName="client_full_name" Name="bctrl_name_full" Sortable="Y" Required="Y" Width="300" DisplayMode="READ"/>
		 -->
		<BizCtrl DisplayName="Тип" FieldName="_cltypes_rid" Name="bctrl__cltypes_rid" SelectFrom="admin.yelowpages.cltypes.BOcltypes[name:Id],[archive]=0" Required="Y" DisplayMode="EDIT|NEW"/>
		<BizCtrl DisplayName="Тип" FieldName="_cltypes_rid" Name="bctrl__cltypes_rid_query" SelectFrom="admin.yelowpages.cltypes.BOcltypes[name:Id],[archive]=0" HTMLAttr="&lt;option value= &gt;Все&lt;/option&gt;" Required="Y" DisplayMode="QUERY"/>

		<BizCtrl DisplayName="Страна" FieldName="_countries_name" Name="bctrl__countries_name" Sortable="Y" DisplayMode="READ"/>		

		<BizCtrl DisplayName="Регион" FieldName="_regions_name" Name="bctrl__regions_name" Sortable="Y" DisplayMode="READ"/>		
		<BizCtrl DisplayName="Город" FieldName="_cities_name" Name="bctrl__cities_name" Sortable="Y" DisplayMode="READ"/>

		<BizCtrl DisplayName="Страна" FieldName="_countries_rid" Name="bctrl__countries_rid" Hidden="N" Required="Y" SelectFrom="admin.yelowpages.countries.BOcountries[name:Id],[archive]=0" DisplayMode="EDIT|NEW">		
			<EventHandler Name="onchange" Event="onchange" Function="UpdateForm()" FunctionType="" />
		</BizCtrl>
		<BizCtrl DisplayName="Регион" FieldName="_regions_rid" Name="bctrl__regions_rid" Required="Y" SelectFrom="admin.yelowpages.regions.BOregions[name:Id],[_countries_rid]={@:Control[bctrl__countries_rid].Value} AND [archive]=0" DisplayMode="EDIT|NEW">		
			<EventHandler Name="onchange" Event="onchange" Function="UpdateForm()" FunctionType="" />
		</BizCtrl>
		<BizCtrl DisplayName="Город" FieldName="_cities_rid" Name="bctrl__cities_rid" SelectFrom="admin.yelowpages.cities.BOcities[name:Id],[_regions_rid]={@:Control[bctrl__regions_rid].Value} AND [archive]=0" Required="Y" DisplayMode="EDIT|NEW"/>

		<BizCtrl DisplayName="Страна" FieldName="_countries_rid" Name="bctrl__countries_rid_query" Hidden="N" Required="Y" SelectFrom="admin.yelowpages.countries.BOcountries[name:Id],[archive]=0" HTMLAttr="&lt;option value= &gt;Все&lt;/option&gt;" DisplayMode="QUERY">		
			<EventHandler Name="onchange" Event="onchange" Function="UpdateForm()" FunctionType="" />
		</BizCtrl>
		<BizCtrl DisplayName="Регион" FieldName="_regions_rid" Name="bctrl__regions_rid_query" Required="Y" SelectFrom="admin.yelowpages.regions.BOregions[name:Id],[_countries_rid]={@:Control[bctrl__countries_rid_query].Value} AND [archive]=0" HTMLAttr="&lt;option value= &gt;Все&lt;/option&gt;" DisplayMode="QUERY">		
			<EventHandler Name="onchange" Event="onchange" Function="UpdateForm()" FunctionType="" />
		</BizCtrl>
		<BizCtrl DisplayName="Город" FieldName="_cities_rid" Name="bctrl__cities_rid_query" SelectFrom="admin.yelowpages.cities.BOcities[name:Id],[_regions_rid]={@:Control[bctrl__regions_rid_query].Value} AND [archive]=0" Required="Y" HTMLAttr="&lt;option value= &gt;Все&lt;/option&gt;" DisplayMode="QUERY"/>

		<BizCtrl DisplayName="Почтовый индекс" FieldName="zip" Name="bctrl_zip" Required="Y" DisplayMode="EDIT|NEW"/>
		<BizCtrl DisplayName="Улица" FieldName="street" Name="bctrl_street" Required="Y" DisplayMode="EDIT|NEW" Width="300"/>
		<BizCtrl DisplayName="Дом, оффис" FieldName="build" Name="bctrl_build" Required="Y" DisplayMode="EDIT|NEW" Width="300"/>
		<BizCtrl DisplayName="Телефоны" FieldName="wphones" Name="bctrl_wphones" Required="Y" DisplayMode="EDIT|NEW" Width="300"/>
		<BizCtrl DisplayName="Skype" FieldName="skype" Name="bctrl_skype" DisplayMode="EDIT|NEW" Width="300"/>
		<BizCtrl DisplayName="Icq" FieldName="icq" Name="bctrl_icq" DisplayMode="EDIT|NEW" Width="300"/>
		<BizCtrl DisplayName="WWW" FieldName="url" Name="bctrl_url" Link="{@:Control[bctrl_url].Value}" Required="Y" DisplayMode="READ|EDIT|NEW" Sortable="Y" Width="300"/>
		<!-- 
		<BizCtrl DisplayName="Рейтинг" FieldName="_cluopinions_mark" Name="bctrl__cluopinions_mark" Required="Y" DisplayMode="READ" Sortable="N"/>
		 -->
		<BizCtrl DisplayName="MSN" FieldName="msn" Name="bctrl_msn" DisplayMode="EDIT|NEW" Width="300"/>
		<BizCtrl DisplayName="Описание" FieldName="descr" Name="bctrl_descr" DisplayMode="EDIT|NEW" Required="Y" Type="Textarea" Width="500" Height="50" />
		<BizCtrl DisplayName="Информация о кредите" FieldName="creadits_info" Name="bctrl_creadits_info" DisplayMode="EDIT|NEW" Type="Textarea" Width="500" Height="50" />
		<BizCtrl DisplayName="Информация о доставке" FieldName="delivery_info" Name="bctrl_delivery_info" DisplayMode="EDIT|NEW" Type="Textarea" Width="500" Height="50" />
		<BizCtrl DisplayName="Режим работы" FieldName="worktime_info" Name="bctrl_worktime_info" Required="Y" DisplayMode="EDIT|NEW" Type="Textarea" Width="500" Height="50" />
		<BizCtrl DisplayName="Дата регистрации" FieldName="reg_date" Name="bctrl_reg_date" Required="Y" DisplayMode="EDIT|NEW" Type="Date"/>
		<BizCtrl DisplayName="Активный" FieldName="active" Name="bctrl_active" Required="N" Sortable="N" Type="listbox" SelectFrom="admin.shared.Selection(NY)" DisplayMode="NEW|EDIT|QUERY"/>
		<BizCtrl DisplayName="Активный" FieldName="active_display_value" Name="bctrl_active_display_value" Required="N" Sortable="N" DisplayMode="READ" />
		<BizCtrl DisplayName="Размещение прайса" FieldName="price_load" Name="bctrl_price_load_query" Sortable="N" HTMLAttr="&lt;option value= &gt;Все&lt;/option&gt;" SelectFrom="admin.shared.Selection(NY)" DisplayMode="QUERY"/>		
		<BizCtrl DisplayName="Размещение прайса" FieldName="price_load" Name="bctrl_price_load" Sortable="N" SelectFrom="admin.shared.Selection(NY)" DisplayMode="EDIT|NEW">
			<EventHandler Name="onchange" Event="onchange" Function="UpdateForm()" FunctionType="" />
		</BizCtrl>
		<BizCtrl DisplayName="Размещение прайса" FieldName="price_load_value" Name="bctrl_price_load_value" Sortable="N" DisplayMode="READ"/>
		
		<BizCtrl DisplayName="Актуальность прайса(дней)" FieldName="pr_actual_days" Enabled="{(@:Control[bctrl_price_load].Value == '1')?'Y':'N'}" Name="bctrl_pr_actual_days" DisplayMode="EDIT|NEW"/>
		<BizCtrl DisplayName="Актуальность прайса(часов)" FieldName="pr_actual_hours" Enabled="{(@:Control[bctrl_price_load].Value == '1')?'Y':'N'}" Name="bctrl_pr_actual_hours" DisplayMode="EDIT|NEW"/>
		<BizCtrl DisplayName="Актуальность прайса(минут)" FieldName="pr_actual_minutes" Enabled="{(@:Control[bctrl_price_load].Value == '1')?'Y':'N'}" Name="bctrl_pr_actual_minutes" DisplayMode="EDIT|NEW"/>
		<BizCtrl DisplayName="E-mail прайса" FieldName="price_email" Enabled="{(@:Control[bctrl_price_load].Value == '1')?'Y':'N'}" Name="bctrl_price_email" DisplayMode="EDIT|NEW" Width="300"/>
		<BizCtrl DisplayName="URL прайса" FieldName="price_url" Enabled="{(@:Control[bctrl_price_load].Value == '1')?'Y':'N'}" Name="bctrl_price_url" DisplayMode="EDIT|NEW" Width="300"/>
		<BizCtrl DisplayName="Контактные телефоны" FieldName="contact_phones" Enabled="{(@:Control[bctrl_price_load].Value == '1')?'Y':'N'}" Name="bctrl_contact_phones" DisplayMode="EDIT|NEW" Width="300"/>
		<BizCtrl DisplayName="Контактный email" FieldName="contact_email" Link="mailto:{@:Control[bctrl_contact_email].Value}" Sortable="N" Enabled="{(@:Control[bctrl_price_load].Value == '1')?'Y':'N'}" Name="bctrl_contact_email" DisplayMode="READ|EDIT|NEW" Width="300"/>
		<BizCtrl DisplayName="Контактное лицо" FieldName="contact_person" Enabled="{(@:Control[bctrl_price_load].Value == '1')?'Y':'N'}" Name="bctrl_contact_person" DisplayMode="EDIT|NEW" Width="300"/>
		<BizCtrl DisplayName="Архивный" FieldName="archive" Name="bctrl_archive" Required="N" Sortable="N" Type="listbox" SelectFrom="admin.shared.Selection(NY)" DisplayMode="NEW|EDIT|QUERY"/>
		<BizCtrl DisplayName="Архивный" FieldName="archive_display_value" Name="bctrl_archive_display_value" Required="N" Sortable="N" DisplayMode="READ" />
		<BizCtrl DisplayName="Создан" FieldName="createDT" Name="bctrl_createDT" Required="Y" Sortable="N" Width="500" DisplayMode="READ" Enabled="N"/>
		<BizCtrl DisplayName="Изменен" FieldName="modifyDT" Name="bctrl_modifyDT" Required="Y" Sortable="N" Width="500" DisplayMode="READ" Enabled="N"/>

	</BizCtrlList>
	<Toolbar>
		<Control Caption="Поиск" DisplayMode="READ" Function="SearchRecord()" Image="search.gif" Name="btn_search" Type="button" />
		<Control Caption="Поиск" DisplayMode="QUERY" Function="RunSearch()" Image="search.gif" Name="btn_dosearch" Type="button" />
		<Control Caption="Правка" DisplayMode="READ" Function="EditRecord()" Image="edit.gif" Name="btn_edit" Type="button" />
		<Control Caption="Новый" DisplayMode="READ" Function="NewRecord()" Image="new.gif" Name="btn_new" Type="button" />
		<Control Caption="Сохранить" DisplayMode="EDIT|NEW" Function="SaveRecord()" Image="save.gif" Name="btn_save" Type="button" />
		<!-- 
		<Control Caption="Копировать" DisplayMode="READ" Function="CopyRecord()" Image="copy.gif" Name="btn_copy" Style="" Type="button"/>
		 -->
		<Control Caption="Удалить" DisplayMode="READ" Function="DeleteRecord()" Image="delete.gif" Name="btn_delete" Style="" Type="button" />
		<Control Caption="Отмена" DisplayMode="EDIT|NEW|QUERY" Function="Cancel()" Image="cancel.gif" Name="btn_cancel" Type="button" />
		<Control Caption="Обновить" DisplayMode="READ" Function="RefreshQuery()" Image="refresh.gif" Name="btn_currency" Style="" Type="button" />
		<Control Caption="Експорт в ексель" Name="btn_excel" Image="excel.gif" Type="button" Width="" Height="" HTMLAttr="" Function="CallService(excelService,renderHTML)" FunctionType="Popup" DisplayMode="READ"/>
		<Control Caption="Експорт" Name="btn_export" Image="" Type="button" HTMLAttr="" Function="CallService(ioService,exportXML)" FunctionType="Page" DisplayMode="READ"/>
		<Control Caption="Импорт" Name="btn_import" Image="" Type="button" HTMLAttr="" Function="ShowPopup(shared.FMImportPopup)" FunctionType="Popup" DisplayMode="READ"/>
	</Toolbar> 
	<Navbar>
		<Control Name="btn_next" Image="next.gif" Caption="Следующая" Type="button" DisplayMode="READ">
			<EventHandler Name="onclick" Event="onclick" Function="NextPage()" />
			<EventHandler Name="nextrecord" Event="" Function="NextRow()" />
		</Control>
		<Control Name="btn_prev" Image="prev.gif" Caption="Предыдущая" Type="button" DisplayMode="READ">
			<EventHandler Name="onclick" Event="onclick" Function="PrevPage()" />
			<EventHandler Name="prevrecord" Event="" Function="PrevRow()" />
		</Control>
			<Control Name="btn_last" Image="nextlast.gif" Caption="В конец" Type="button" Width="" Height="" HTMLAttr="" Function="GotoPage(-1)" Style="" Enabled="" DisplayMode="" Access=""/>
			<Control Name="btn_first" Image="prevfirst.gif" Caption="В начало" Type="button" Width="" Height="" HTMLAttr="" Function="GotoPage(1)" Style="" Enabled="" DisplayMode="" Access=""/>
	</Navbar> 
</BizForm>
 